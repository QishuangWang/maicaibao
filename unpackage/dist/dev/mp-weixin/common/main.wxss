	width: 153.6rpx;
	height: 72rpx;
	-webkit-transition-property: background-color, border;
	transition-property: background-color, border;
	border: 4.8rpx solid #ddd;
	border-radius: 48rpx;
	background-color: #fff;
	box-sizing: border-box;
	position: relative;
}
.switch-left,
.switch-right {
	width: 100%;
	visibility: hidden;
	line-height: 72rpx;
	color: #646464;
}
.switch-left {
	position: absolute;
	right: 0rpx;
	text-align: left;
	top: -4.8rpx;
	left: -4.8rpx;
	bottom: 0rpx;
	padding-left: 24rpx;
}
.switch-right {
	position: absolute;
	right: 0rpx;
	text-align: right;
	top: -4.8rpx;
	left: -4.8rpx;
	bottom: 0rpx;
	padding-right: 24rpx;

	padding-left: 72rpx;
}
.switch-left:after,
.switch-right:before {
	content: ".";
	width: 72rpx;
	height: 72rpx;
	background-color: #007BFF;
	border-radius: 48rpx;
	color: #007BFF;
	position: absolute;

	top: 0rpx;
}
.switch-left:after {
	right: -4.8rpx;
}
.switch-right:before {
	left: -4.8rpx;
}
.switch-active {
	visibility: visible;
} 
